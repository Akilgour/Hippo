@inherits ErrorLogPagedListTableBase

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>TimeStamp</th>
            <th>Message</th>
            <th>Machine Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ErrorLogPagedList.ErrorLogs)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.TimeStamp</td>
                <td>@item.Message</td>
                <td>@item.MachineName</td>
            </tr>
        }
    </tbody>
</table>