@page "/perflogoverview"
@inherits PerfLogOverviewBase
@using Hippologamus.Server.Components

<h3>PerfLogOverview</h3>

@if (PerfLogAssemblyDisplays == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var perfLogAssembly in PerfLogAssemblyDisplays)
            {
                <PerfLogAssemblyRow @key="perfLogAssembly" PerfLogAssembly="perfLogAssembly"></PerfLogAssemblyRow>
            }
        </tbody>
    </table>

}